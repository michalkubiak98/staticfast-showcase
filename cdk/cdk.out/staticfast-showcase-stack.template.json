  "TrailingSlashFunction764C8588": {
   "Type": "AWS::CloudFront::Function",
   "Properties": {
    "AutoPublish": true,
    "FunctionCode": "\n        function handler(event) {\n          var request = event.request;\n          var uri = request.uri;\n          \n          // Check whether the URI is missing a file extension\n          if (!uri.includes('.')) {\n            // Check if URI already ends with /\n            if (!uri.endsWith('/')) {\n              // Add trailing slash\n              request.uri = uri + '/';\n            }\n            // Append index.html to the URI\n            request.uri = request.uri + 'index.html';\n          }\n          \n          return request;\n        }\n      ",
    "FunctionConfig": {
     "Comment": "staticfast-showcase-trailing-slash",
     "Runtime": "cloudfront-js-1.0"
    },
    "Name": "staticfast-showcase-trailing-slash"
   },
